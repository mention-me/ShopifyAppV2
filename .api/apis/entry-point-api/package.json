{
  "name": "@api/entry-point-api",
  "version": "2.0.0",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "module": "dist/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./sdk": {
      "import": "./dist/sdk.js",
      "require": "./dist/sdk.cjs"
    },
    "./types": {
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "openapi.json"
  ],
  "scripts": {
    "lint": "tsc --noEmit",
    "prepare": "tsup"
  },
  "dependencies": {
    "@readme/api-core": "^7.0.0-beta.6"
  },
  "devDependencies": {
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "tsup": {
    "cjsInterop": true,
    "clean": true,
    "dts": true,
    "entry": [
      "./src/index.ts",
      "./src/sdk.ts",
      "./src/types.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "minify": false,
    "shims": true,
    "sourcemap": true,
    "splitting": true
  }
}
